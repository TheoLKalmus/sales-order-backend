{
  "compilerOptions": {
    "target": "ES2024",
    "module": "Node16",
    "moduleResolution": "Node16",
    "strict": true,
    "strictNullChecks": true, 
    "noImplicitAny": true,
    "esModuleInterop": false,
    "outDir": "./dist",
    "baseUrl": ".",               // Corrigido aqui
    "rootDir": "./",
    "paths": {
      "@models/*": ["@cds-models/*/index.ts"],
      "@cds-models/*": ["@cds-models/*"]
    }
  },
  "include": ["srv", "@cds-models"],
  "exclude": ["node_modules"]
}